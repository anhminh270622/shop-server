{
	"functions": {
		"api/server.js": {
			"memory": 1024,
			"includeFiles": "db.json"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "api/server.js"
		}
	],
	"routes": [
		{ "src": "/api/order", "methods": ["GET", "POST", "DELETE"] },
		{ "src": "/api/contact", "methods": ["GET", "POST", "DELETE"] },
		{ "src": "/api/user", "methods": ["GET", "POST", "DELETE"] }
	]
}
